<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alphanese.ezsoc5">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ezsoc"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Splashscreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dr"
            android:label="@string/title_activity_dr"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".tr"
            android:label="@string/title_activity_tr"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sr"
            android:label="@string/title_activity_sr"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AdvSearch" />

        <activity android:name=".AvailSearch"></activity>
    </application>

</manifest>